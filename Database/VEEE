SELECT pr.pro_clave, pr.pro_nombre
FROM pedido p, detalle_pedido d, producto_tienda t, producto pr
WHERE   p.ped_numero_compra=d.ped_numero_compra
        AND t.pt_id=d.pt_id
        AND t.pro_clave=pr.pro_clave
        
        
