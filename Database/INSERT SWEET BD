--PEDIDO-ESTATUS
INSERT INTO public.pedido_estatus(ped_numero_compra, est_clave, pe_fecha_hora_inicio)
	VALUES ((select ped_numero_compra FROM pedido WHERE fk_usuario in (select usu_id FROM USUARIO WHERE usu_username LIKE'%gdpalacios.20%')),
            (select est_clave FROM estatus WHERE est_tipo LIKE '%Presupuesto%'), '20/07/2022');

--DETALLE PEDIDO

INSERT INTO public.detalle_pedido(
	ped_numero_compra, pt_id, dp_cantidad, dp_precio, dp_peso_total)
	VALUES ((select ped_numero_compra 
             FROM pedido 
             WHERE fk_usuario in (select usu_id FROM USUARIO WHERE usu_username LIKE'%gdpalacios.20%')),
             (select pt_id 
              from producto_tienda 
              where pro_clave in (select pro_clave 
                                   from producto 
                                   where pro_nombre LIKE'%takis%')
              and tie_clave in (select fk_tienda 
                                FROM pedido 
                                WHERE fk_usuario in (select usu_id FROM USUARIO WHERE usu_username LIKE'%gdpalacios.20%' ))),
             4,
             (select p.pro_precio_actual 
              FROM producto p
              WHERE p.pro_nombre LIKE '%takis%'), 
              NULL) RETURNING *;
              